<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="advance_salary_sequence" model="ir.sequence">
        <field name="name">Advance Salary</field>
        <field name="code">hr.advance.salary</field>
        <field name="prefix">%(month)s-%(year)s -</field>
        <field name="padding">4</field>
        <field eval="1" name="number_increment"/>
    </record>

    <record id="request_loan_sequence" model="ir.sequence">
        <field name="name">Request loan</field>
        <field name="code">hr.loaning.request</field>
        <field name="prefix">%(month)s-%(year)s -</field>
        <field name="padding">4</field>
        <field eval="1" name="number_increment"/>
    </record>

    <record id="due_date_sequence" model="ir.sequence">
        <field name="name">Due date</field>
        <field name="code">hr.due.date</field>
        <field name="prefix">%(month)s-%(year)s -</field>
        <field name="padding">4</field>
        <field eval="1" name="number_increment"/>
    </record>
</odoo>